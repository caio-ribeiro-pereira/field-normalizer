{
  "name": "field-normalizer",
  "description": "A tiny lib for object field's normalization",
  "version": "0.0.7",
  "main": "index.js",
  "license": "MIT",
  "author": "Caio Ribeiro Pereira <caio.ribeiro.pereira@gmail.com>",
  "scripts": {
    "test": "jest test/**",
    "coverage": "jest test/** --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint . --fix"
  },
  "repository": "https://github.com/caio-ribeiro-pereira/field-normalizer",
  "keywords": [
    "object",
    "field",
    "key",
    "normalizer",
    "object-transform",
    "object-normalization",
    "transform"
  ],
  "devDependencies": {
    "coveralls": "3.1.0",
    "eslint": "7.12.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.6.1"
  },
  "dependencies": {
    "change-case": "4.1.1"
  },
  "engines": {
    "node": ">= 11.0.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**"
    ]
  }
}
